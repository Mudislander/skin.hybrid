<?xml version="1.0" encoding="utf-8"?>
<includes>
    <include name="Lowlist">
        <control type="group">
            <visible>Control.IsVisible(724)</visible>
            <include>VisibleFadeEffect</include>
            <control type="list" id="724">
                <left>340</left>
                <top>440</top>
                <width>580</width>
                <height>160</height>
                <onleft>9000</onleft>
                <onright>72460</onright>
                <onup>724</onup>
                <ondown>724</ondown>
                <viewtype label="$LOCALIZE[31653]">list</viewtype>
                <pagecontrol>72460</pagecontrol>
                <scrolltime>200</scrolltime>
                <visible>Container.Content(Movies) | Container.Content(Seasons) | Container.Content(TVShows) | Container.Content(MusicVideos)</visible>
                <visible>!Container.Content(LiveTV)</visible>
                <itemlayout height="40" width="480">
                    <control type="image">
                        <left>0</left>
                        <top>0</top>
                        <width>480</width>
                        <height>41</height>
                        <texture border="0,2,0,2">MenuItemNF.png</texture>
                    </control>
                    <control type="image">
                        <left>0</left>
                        <top>6</top>
                        <width>28</width>
                        <height>28</height>
                        <texture diffuse="poster-diffuse.png" colordiffuse="$VAR[MainColorOne]">$VAR[PartlyWatched,MediaStatus/,.png]</texture>
                        <visible>!Skin.HasSetting(DisableWatchedCheck) + !IsEmpty(ListItem.Overlay)</visible>
                    </control>
                    <control type="label">
                        <left>30</left>
                        <top>0</top>
                        <width>440</width>
                        <height>40</height>
                        <font>font13</font>
                        <textcolor>grey2</textcolor>
                        <selectedcolor>$VAR[SelectedColor]</selectedcolor>
                        <align>left</align>
                        <aligny>center</aligny>
                        <label>$INFO[ListItem.Label]</label>
                    </control>
                    <control type="label">
                        <left>100</left>
                        <top>0</top>
                        <width>360</width>
                        <height>40</height>
                        <font>font12</font>
                        <textcolor>grey2</textcolor>
                        <selectedcolor>$VAR[SelectedColor]</selectedcolor>
                        <align>right</align>
                        <aligny>center</aligny>
                        <label>$INFO[ListItem.Label2]</label>
                    </control>
                </itemlayout>
                <focusedlayout height="40" width="480">
                    <control type="image">
                        <left>0</left>
                        <top>0</top>
                        <width>480</width>
                        <height>41</height>
                        <texture border="0,2,0,2">MenuItemNF.png</texture>
                        <visible>!Control.HasFocus(724)</visible>
                        <include>VisibleFadeEffect</include>
                    </control>
                    <control type="image">
                        <left>0</left>
                        <top>0</top>
                        <width>470</width>
                        <height>41</height>
                        <texture border="0,2,0,2" colordiffuse="$VAR[MainColorOne]">MenuItemFO.png</texture>
                        <visible>Control.HasFocus(724)</visible>
                        <include>VisibleFadeEffect</include>
                    </control>
                    <!--control type="image">
						<left>370</left>
						<top>5</top>
						<width>100</width>
						<height>31</height>
						<texture border="0,0,14,0" colordiffuse="$VAR[MainColorOne]">MediaItemDetailBG.png</texture>
						<visible>Control.HasFocus(724)</visible>
					</control-->
                    <control type="image">
                        <left>0</left>
                        <top>6</top>
                        <width>28</width>
                        <height>28</height>
                        <texture diffuse="poster-diffuse.png" colordiffuse="$VAR[MainColorOne]">$VAR[PartlyWatched,MediaStatus/,.png]</texture>
                        <visible>!Skin.HasSetting(DisableWatchedCheck) + !IsEmpty(ListItem.Overlay)</visible>
                    </control>
                    <control type="label">
                        <left>30</left>
                        <top>0</top>
                        <width>440</width>
                        <height>40</height>
                        <font>font13</font>
                        <textcolor>$VAR[MainColorTwo]</textcolor>
                        <selectedcolor>$VAR[SelectedColor]</selectedcolor>
                        <align>left</align>
                        <aligny>center</aligny>
                        <label>$INFO[ListItem.Label]</label>
                    </control>
                    <control type="label">
                        <left>100</left>
                        <top>0</top>
                        <width>360</width>
                        <height>40</height>
                        <font>font12</font>
                        <textcolor>grey2</textcolor>
                        <selectedcolor>$VAR[SelectedColor]</selectedcolor>
                        <align>right</align>
                        <aligny>center</aligny>
                        <label>$INFO[ListItem.Label2]</label>
                    </control>
                </focusedlayout>
            </control>
            <control type="scrollbar" id="72460">
                <left>810</left>
                <top>440</top>
                <width>25</width>
                <height>160</height>
                <texturesliderbackground border="0,14,0,14">ScrollBarV.png</texturesliderbackground>
                <texturesliderbar border="0,14,0,14">ScrollBarV_bar.png</texturesliderbar>
                <texturesliderbarfocus border="0,14,0,14" colordiffuse="$VAR[MainColorOne]">ScrollBarV_bar_focus.png</texturesliderbarfocus>
                <textureslidernib>ScrollBarNib.png</textureslidernib>
                <textureslidernibfocus colordiffuse="$VAR[MainColorOne]">ScrollBarNib.png</textureslidernibfocus>
                <onleft>724</onleft>
                <onright>800</onright>
                <showonepage>true</showonepage>
                <orientation>vertical</orientation>
                <visible>Control.IsVisible(724)</visible>
            </control>
            <control type="group">
                <visible>Control.IsVisible(724)</visible>
                <control type="group">
                    <visible>IsEmpty(ListItem.Art(poster)) | !Window.IsVisible(Videos)</visible>
                    <left>40</left>
                    <top>185</top>
                    <camera x="640" y="200" />
                    <animation effect="rotatey" center="50" start="-30" end="-30" time="0" condition="true">Conditional</animation>
                    <control type="image">
                        <left>0</left>
                        <top>0</top>
                        <width>260</width>
                        <height>480</height>
                        <aspectratio aligny="bottom">keep</aspectratio>
                        <fadetime>IconCrossfadeTime</fadetime>
                        <texture background="true" colordiffuse="$VAR[MainColorOne]">DefaultLogoReturn.png</texture>
                        <visible>stringcompare(ListItem.Label,..)</visible>
                    </control>
                    <control type="image">
                        <left>0</left>
                        <top>0</top>
                        <width>260</width>
                        <height>480</height>
                        <aspectratio aligny="bottom">keep</aspectratio>
                        <fadetime>IconCrossfadeTime</fadetime>
                        <texture background="true">$INFO[ListItem.Thumb]</texture>
                        <visible>!stringcompare(ListItem.Label,..)</visible>
                    </control>
                </control>
                <control type="group">
                    <visible>!IsEmpty(ListItem.Art(poster)) + Window.IsVisible(Videos)</visible>
                    <left>40</left>
                    <top>185</top>
                    <camera x="640" y="200" />
                    <animation effect="rotatey" center="50" start="-30" end="-30" time="0" condition="true">Conditional</animation>
                    <control type="image">
                        <left>0</left>
                        <top>-15</top>
                        <width>260</width>
                        <height>480</height>
                        <aspectratio aligny="bottom">keep</aspectratio>
                        <fadetime>IconCrossfadeTime</fadetime>
                        <texture background="true" diffuse="ClearCase/EmptyCaseMask.png">$INFO[ListItem.Art(poster)]</texture>
                        <visible>!Skin.HasSetting(EnableDVDCases)</visible>
                    </control>
                    <control type="image">
                        <left>33</left>
                        <top>15</top>
                        <width>217</width>
                        <height>445</height>
                        <aspectratio scalediffuse="false">stretch</aspectratio>
                        <fadetime>IconCrossfadeTime</fadetime>
                        <texture background="true" diffuse="ClearCase/EmptyCaseMask.png">$INFO[ListItem.Art(poster)]</texture>
                        <visible>Skin.HasSetting(EnableDVDCases)</visible>
                    </control>
                    <control type="image">
                        <left>0</left>
                        <top>0</top>
                        <width>260</width>
                        <height>480</height>
                        <aspectratio>stretch</aspectratio>
                        <texture fallback="ClearCase/dv.png">$VAR[VideoListCase,ClearCase/,.png]</texture>
                        <visible>Skin.HasSetting(EnableDVDCases)</visible>
                    </control>
                </control>
                <control type="grouplist">
                    <description>Media Codec Flagging Images</description>
                    <left>220</left>
                    <top>105r</top>
                    <width>151</width>
                    <align>left</align>
                    <itemgap>1</itemgap>
                    <orientation>horizontal</orientation>
                    <include condition="!Skin.HasSetting(EnableFullMPAA)">VideoMPAAFlaggingConditions</include>
                    <include>VideoStudioFlagging</include>
                    <include>MovieSetFlaggingConditions</include>
                    <visible>!IsEmpty(ListItem.Title)</visible>
                </control>
                <control type="grouplist">
                    <description>Media Codec Flagging Images</description>
                    <left>530</left>
                    <top>620</top>
                    <width>275</width>
                    <align>right</align>
                    <itemgap>1</itemgap>
                    <orientation>horizontal</orientation>
                    <include>VideoCodecFlaggingConditions</include>
                    <include>AudioCodecFlaggingConditions</include>
                    <include>AudioChannelsFlaggingConditions</include>
                    <include>AspectCodecFlaggingConditions</include>
                    <visible>!IsEmpty(ListItem.Title)</visible>
                </control>
                <control type="textbox">
                    <description>Description Value for Movie</description>
                    <left>840</left>
                    <top>440</top>
                    <width>420</width>
                    <height>160</height>
                    <font>font12</font>
                    <align>justify</align>
                    <textcolor>$VAR[MainColorOne]</textcolor>
                    <label>$VAR[VideoDescription]</label>
                    <autoscroll time="3600" delay="6000" repeat="6000">!Skin.HasSetting(DisableAutoScroll)</autoscroll>
                    <visible>!StringCompare(ListItem.Label,..)</visible>
                </control>
                <control type="image">
                    <description>Rating value</description>
                    <left>840</left>
                    <top>620</top>
                    <width>150</width>
                    <height>25</height>
                    <aspectratio align="left">keep</aspectratio>
                    <texture colordiffuse="lightgrey">LeftRating/$INFO[ListItem.StarRating]</texture>
                    <visible>!IsEmpty(ListItem.StarRating)</visible>
                </control>
                <control type="label">
                    <right>30</right>
                    <top>620</top>
                    <width>300</width>
                    <height>30</height>
                    <font>font10</font>
                    <textcolor>$VAR[MainColorOne]</textcolor>
                    <shadowcolor>black</shadowcolor>
                    <scroll>false</scroll>
                    <align>right</align>
                    <aligny>top</aligny>
                    <label>$VAR[MediaLastPlayed]</label>
                    <visible>!IsEmpty(ListItem.LastPlayed)</visible>
                </control>
                <control type="label">
                    <right>30</right>
                    <top>620</top>
                    <width>300</width>
                    <height>30</height>
                    <font>font10</font>
                    <textcolor>$VAR[MainColorOne]</textcolor>
                    <scroll>false</scroll>
                    <align>right</align>
                    <aligny>top</aligny>
                    <label>$VAR[MediaRuntime,[B]$LOCALIZE[2050]: [/B]]</label>
                    <visible>IsEmpty(ListItem.LastPlayed) + !IsEmpty(ListItem.Duration)</visible>
                </control>
            </control>
            <include>VideoLogoArt</include>
        </control>
    </include>
</includes>